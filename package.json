{
  "name": "mp-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "PRIVATE",
  "scripts": {
    "prebuild": "rimraf dist",
    "postinstall": "npx prisma migrate deploy --schema=./libs/prisma/src/prisma/schema.prisma && npx prisma generate --schema=./libs/prisma/src/prisma/schema.prisma && nest build hp-fence",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "DEBUG=false node dist/apps/hp-fence/main.js",
    "start:dev-hp-fence": "export $(cat .env | xargs) && nest start hp-fence --watch"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.3",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/swagger": "^5.1.5",
    "@nestjs/throttler": "^2.0.1",
    "@prisma/client": "^3.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/crypto-js": "^4.0.2",
    "@types/websocket": "^1.0.4",
    "apollo-server-core": "^3.10.2",
    "apollo-server-express": "^3.10.3",
    "axios": "^1.6.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "express-handlebars": "^7.1.2",
    "fastify-swagger": "^4.12.6",
    "graphql": "^15.5.1",
    "hbs": "^4.2.0",
    "js-sha256": "^0.9.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "nestjs-i18n": "^8.1.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.1.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.5",
    "swagger-ui-express": "^4.2.0",
    "twilio": "^3.75.1",
    "uuid": "^8.3.2",
    "websocket": "^1.0.34",
    "whatsapp": "^0.0.5-Alpha",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "prisma": "^3.7.0",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/helpers(|/.*)$": "<rootDir>/libs/helpers/src/$1",
      "^@app/validations(|/.*)$": "<rootDir>/libs/validations/src/$1",
      "^@app/errors(|/.*)$": "<rootDir>/libs/errors/src/$1",
      "^@app/services(|/.*)$": "<rootDir>/libs/services/src/$1",
      "^@app/websocket(|/.*)$": "<rootDir>/libs/websocket/src/$1"
    }
  }
}
